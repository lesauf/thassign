{
  "name": "thassign",
  "version": "0.0.0",
  "scripts": {
    "android": "tns run android --no-hmr",
    "ios": "tns run ios --no-hmr",
    "mobile": "tns run",
    "preview": "tns preview",
    "clean": "npx rimraf hooks node_modules package-lock.json platforms",
    "ngcc": "ngcc --properties es2015 module main --first-only",
    "postinstall": "npm run ngcc",
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --port 8080 --disable-host-check true --poll=2000",
    "build": "ng build",
    "test": "jest",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test:watch": "jest auth.service.spec.ts --watch"
  },
  "author": "Fotso Teketchuet Stephane",
  "private": true,
  "node": {
    "net": "empty",
    "tls": "empty",
    "dns": "empty"
  },
  "dependencies": {
    "@angular/animations": "~15.2.10",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "~15.2.10",
    "@angular/compiler": "~15.2.10",
    "@angular/core": "~15.2.10",
    "@angular/fire": "^6.1.5",
    "@angular/flex-layout": "^12.0.0-beta.34",
    "@angular/forms": "~15.2.10",
    "@angular/material": "^15.2.9",
    "@angular/platform-browser": "~15.2.10",
    "@angular/platform-browser-dynamic": "~15.2.10",
    "@angular/router": "~15.2.10",
    "@fortawesome/angular-fontawesome": "^0.9.0",
    "@nativescript/angular": "^12.0.6",
    "@nativescript/core": "^8.0.8",
    "@nativescript/theme": "^3.0.1",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "class-validator": "^0.14.0",
    "core-js": "^3.16.2",
    "epubjs": "^0.3.90",
    "firebase": "^8.10.0",
    "luxon": "^2.0.2",
    "messageformat": "^2.3.0",
    "nativescript-plugin-firebase": "^5.2.0",
    "nativescript-pro-ui": "^3.4.1",
    "ngx-translate-messageformat-compiler": "^4.10.0",
    "postcss": "^8.3.11",
    "reflect-metadata": "~0.1.13",
    "rxjs": "^7.3.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1202.2",
    "@angular-devkit/build-angular": "^15.2.10",
    "@angular/cli": "^15.2.10",
    "@angular/compiler-cli": "~15.2.10",
    "@angular/language-service": "~15.2.10",
    "@nativescript/schematics": "^11.2.0",
    "@nativescript/tslint-rules": "~0.0.5",
    "@nativescript/types": "^8.0.1",
    "@nativescript/webpack": "^5.0.18",
    "@ngneat/spectator": "^8.0.3",
    "@ngtools/webpack": "^12.2.2",
    "@types/faker": "^5.5.8",
    "@types/jest": "^27.0.1",
    "@types/luxon": "^2.0.0",
    "@types/node": "^16.6.2",
    "codelyzer": "^0.0.28",
    "copy-webpack-plugin": "^9.0.1",
    "faker": "^5.5.3",
    "firebase-tools": "^13.0.1",
    "fuzzy": "^0.1.3",
    "inquirer": "^8.1.2",
    "inquirer-autocomplete-prompt": "^1.4.0",
    "jest": "^27.0.6",
    "jest-preset-angular": "^9.0.6",
    "open": "^8.2.1",
    "protractor": "^3.3.0",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "tslint": "^6.1.0",
    "typescript": "~4.9.5"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "roots": [
      "src"
    ],
    "transform": {
      "^.+\\.(ts|js|html)$": "ts-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/setup-jest.ts"
    ],
    "collectCoverage": true,
    "moduleNameMapper": {
      "@app/(.*)": "<rootDir>/src/app/$1",
      "@assets/(.*)": "<rootDir>/src/assets/$1",
      "@core/(.*)": "<rootDir>/src/app/core/$1",
      "@env": "<rootDir>/src/environments/environment",
      "@src/(.*)": "<rootDir>/src/src/$1",
      "@state/(.*)": "<rootDir>/src/app/state/$1"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/tsconfig.spec.json",
        "stringifyContentPathRegex": "\\.html$",
        "astTransformers": [
          "jest-preset-angular/build/InlineFilesTransformer",
          "jest-preset-angular/build/StripStylesTransformer"
        ]
      }
    }
  },
  "nativescript": {
    "id": "com.lesauf.thassign",
    "tns-android": {
      "version": "6.5.3"
    }
  }
}